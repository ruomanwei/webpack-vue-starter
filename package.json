{
  "name": "vue-ui-tree",
  "version": "1.0.0",
  "description": "一款基于webpack 和 vue 简单的脚手架，方便快速开发，支持es2015",
  "main": "dist/vue-tree/main.js",
  "scripts": {
    "dev": "webpack-dev-server --open --inline --hot --compress --history-api-fallback --port 8080 --config build/dev-server.js",
    "dist:dev": "SET NODE_ENV=development&& webpack -d --watch --color --config build/webpack.dev.config.js",
    "deploy": "SET NODE_ENV=production&& webpack -p --colors --config build/webpack.dev.config.js"
  },
  "keywords": [
    "vue"
  ],
  "peerDependencies": {
    "vue": "^2.1.10"
  },
  "devDependencies": {
    "axios": "^0.17.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.7",
    "es6-promise": "^4.1.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.2.1",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "vue": "^2.2.1",
    "vue-hot-reload-api": "^1.2.3",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^11.0.0",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^3.0.0"
  },
  "author": "daniel chan",
  "license": "MIT"
}
